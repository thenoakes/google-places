    "bulma": "^0.7.4",
    "dotenv": "^7.0.0",
    "express": "^4.17.3",
    "geolib": "^2.0.24",
    "is-valid-coordinates": "^1.0.0",
    "opn": "^5.5.0",
    "raz": "^1.4.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "mocha": "^9.2.2"
  }

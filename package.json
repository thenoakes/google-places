{
  "name": "google-places",
  "version": "1.0.0",
  "description": "An application that accepts a location as input, and presents a list of places matching certain criteria",
  "main": "index.js",
  "scripts": {
    "start": "npm install && node index.js",
    "test": "npm install && mocha",
    "start-env": "op run --no-masking --env-file .env -- npm start",
